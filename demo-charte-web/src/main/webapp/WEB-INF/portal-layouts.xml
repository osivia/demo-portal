<?xml version="1.0" encoding="UTF-8"?>
<layouts>

    <layout>
        <name>demo-web-2-cols</name>
        <uri>/layouts/2-cols/index.jsp</uri>
        <uri state="maximized">/layouts/2-cols/maximized.jsp</uri>
        <regions>
            <region name="top" />
            <region name="col-1-before" />
            <region name="col-1" />
            <region name="col-1-after" />
            <region name="col-2-before" />
            <region name="col-2" />
            <region name="col-2-after" />
            <region name="bottom" />
        </regions>
    </layout>
    
    <layout>
        <name>demo-web-3-cols</name>
        <uri>/layouts/3-cols/index.jsp</uri>
        <uri state="maximized">/layouts/3-cols/maximized.jsp</uri>
        <regions>
            <region name="top" />
            <region name="col-1-before" />
            <region name="col-1" />
            <region name="col-1-after" />
            <region name="col-2-before" />
            <region name="col-2" />
            <region name="col-2-after" />
            <region name="col-3-before" />
            <region name="col-3" />
            <region name="col-3-after" />
            <region name="bottom" />
        </regions>
    </layout>

</layouts>
