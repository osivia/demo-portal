    <div class="footer-links">
        <div class="container">
            <p:region regionName="footer-links" cms="true" />
        </div>
    </div>
    
    <!-- Copyright -->
    <div class="copyright">
        <div class="container">
            <p class="text-center">
                <i class="glyphicons glyphicons-copyright-mark"></i>
                <span>OSIVIA - 2018</span>
            </p>
        </div>
    </div>
</footer>


<!-- Notifications -->
<p:region regionName="notifications" />
<!-- Page settings -->
<p:region regionName="pageSettings" />
<!-- AJAX scripts -->
<p:region regionName="AJAXScripts" />
<!-- AJAX footer -->
<p:region regionName="AJAXFooter" />
