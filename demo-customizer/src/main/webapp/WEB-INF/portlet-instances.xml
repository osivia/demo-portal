<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE deployments PUBLIC "-//JBoss Portal//DTD Portlet Instances 2.6//EN" "http://www.jboss.org/portal/dtd/portlet-instances_2_6.dtd">

<deployments>

    <deployment>
        <if-exists>overwrite</if-exists>
        <instance>
            <instance-id>demo-customizer-instance</instance-id>
            <portlet-ref>demo-customizer</portlet-ref>
            <preferences>
                <preference>
                    <name>hide</name>
                    <value>true</value>
                </preference>
            </preferences>
        </instance>
    </deployment>

</deployments>
